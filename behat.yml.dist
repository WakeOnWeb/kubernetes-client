default:
    suites:
        application:
            contexts:
                - ClientContext:
                    - https://baseUrl # Base server URL
                    - v1              # API version
                    - username        # Username
                    - password        # Password
                - NamespaceContext
                - SecretContext
                - ServiceContext
                - PodContext
                - PersistentVolumeContext

    gherkin:
        filters:
            tags: "~@wip"
